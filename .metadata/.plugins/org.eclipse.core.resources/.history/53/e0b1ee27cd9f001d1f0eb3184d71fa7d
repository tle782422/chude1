                            '                                    <path d="M13.5 5.5c1.1 0 2-.9 2-2s-.9-2-2-2-2 .9-2 2 .9 2 2 2zM9.8 8.9L7 23h2.1l1.8-8 2.1 2v6h2v-7.5l-2.1-2 .6-3C14.8 12 16.8 13 19 13v-2c-1.9 0-3.5-1-4.3-2.4l-1-1.6c-.4-.6-1-1-1.7-1-.3 0-.5.1-.8.1L6 8.3V13h2V9.6l1.8-.7"></path>' +
                            '                                </svg>' +
                            '                            </span>' +
                            '                            <span class="txt">Truyện Theo Dõi</span>' +
                            '                        </div>' +
                            '                        <div class="action user-history">' +
                            '                            <span class="icon">' +
                            '                                <svg><path d="M13 3c-4.97 0-9 4.03-9 9H1l3.89 3.89.07.14L9 12H6c0-3.87 3.13-7 7-7s7 3.13 7 7-3.13 7-7 7c-1.93 0-3.68-.79-4.94-2.06l-1.42 1.42C8.27 19.99 10.51 21 13 21c4.97 0 9-4.03 9-9s-4.03-9-9-9zm-1 5v5l4.28 2.54.72-1.21-3.5-2.08V8H12z"></path></svg>' +
                            '                            </span>' +
                            '                            <span class="txt">Truyện Đã Đọc</span>' +
                            '                        </div>' +
                            '                        <div class="action user-logout">' +
                            '                            <span class="icon">' +
                            '                                <svg>' +
                            '                                    <path d="M10.09 15.59L11.5 17l5-5-5-5-1.41 1.41L12.67 11H3v2h9.67l-2.58 2.59zM19 3H5c-1.11 0-2 .9-2 2v4h2V5h14v14H5v-4H3v4c0 1.1.89 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2z"></path>' +
                            '                                </svg>' +
                            '                            </span>' +
                            '                            <span class="txt">Đăng Xuất</span>' +
                            '                        </div>' +
                            '                    </div>';
                    }
                    $(modal.loginTab).html(html);
                }
            },
            doSignOut: () => {
                const confirm = window.confirm('Bạn có muốn đăng xuất ?');
                if (confirm) {
                    $.ajax({
                        url: "/api/cleanSession",
                        type: 'POST'
                    }).done(res => {
                        if (res.status && res.result) {
                            // Clear store user
                            const user = getUserData();
                            if (user.type === "FB" && !!window.FB) {
                                try {
                                    window.FB.logout();
                                } catch (e) {
                                    window.FB.logout;
                                }
                            }
                            clearTokenUser();
                            window.location.reload();
                        }
                    });
                }
            },
            doRestoreDeviceData: () => {
                $.ajax({
                    url: `/api/user/restoreData`,
                    beforeSend: beforeAuth,
                    type: 'POST',
                    dataType: "json"
                }).done(res => {
                    if (res.status === true) {
                        const data = res.result;
                        const isAgree = confirm(data.message);
                        if (isAgree) {
                            modal.storeLocalSession(data.localSessionId);
                            localStorage.setItem("history_info", data.historyData);
                            localStorage.setItem("tracking-chapter", data.trackingData);
                            modal.showNotification('Đồng bộ dữ liệu xuống thiết bị thành công', modal.notificationInfo);
                        }
                    } else {
                        modal.showNotification(res.messages[0], modal.notificationInfo);
                    }
                });
            },
            doSyncPush: () => {
                // Sync push
                try {
                    const status = Notification.permission;
                    if (status === "denied") {
                        modal.showNotification("Trình duyệt của bạn đã chặn nhận thông báo, bạn vui lòng mở cho phép ở thanh địa chỉ.", modal.notificationError);
                    } else {
                        const addFunc = (state, res) => {
                            if (res.status && res.result) {
                                modal.showNotification("Bạn đã đồng bộ thông báo thành công", modal.notificationSuccess);
                            } else {
                                modal.showNotification(res.messages[0], modal.notificationInfo);
                            }
                        };
                        const denied = (state) => {
                            if (state === "denied") {
                                modal.showNotification("Trình duyệt của bạn đã chặn nhận thông báo, bạn vui lòng mở cho phép ở thanh địa chỉ.", modal.notificationInfo);
                            }
                        };
                        registerPushNotification((token) => {
                            return $.ajax({
                                url: `/api/push/sync`,
                                beforeSend: beforeAuth,
                                type: 'POST',
                                dataType: "json",
                                data: {
                                    comicId: '',
                                    token: token,
                                    sessionId: modal.getLocalSession()
                                }
                            });
                        }, addFunc, denied);
                    }
                } catch (e) {
                    modal.showNotification("Trình duyệt của bạn không có hổ trợ.", modal.notificationInfo);
                }
                // Sync account
                $.ajax({
                    url: `/api/user/syncData`,
                    beforeSend: beforeAuth,
                    type: 'POST',
                    dataType: "json",
                    data: {
                        localSessionId: modal.getLocalSession(),
                        historyData: modal.getHistoryData(),
                        trackingData: modal.getTrackingData()
                    }
                }).done(res => {
                    if (res.status === true) {
                        modal.showNotification('Đồng bộ dữ liệu lên tài khoản thành công ', modal.notificationSuccess);
                        modal.storeLocalSession(res.result.localSessionId)
                    }
                });
            },
            getLocalSession: () => {
                return localStorage.getItem("localSession");
            },
            storeLocalSession: (session) => {
                localStorage.setItem("localSession", session);
            },
            getHistoryData: () => {
                return localStorage.getItem("history_info");
            },
            getTrackingData: () => {
                return localStorage.getItem("tracking-chapter");
            },
            checkReportFixed: () => {
                $.ajax({
                    url: `/api/chapter/reportFixed`,
                    beforeSend: beforeAuth,
                    type: 'POST',
                    dataType: "json",
                    data: {
                        localSessionId: modal.getLocalSession()
                    }
                }).done(res => {
                    if (res.status && res.result.state) {
                        modal.showNotification(res.result.msg, modal.notificationInfo);
                        modal.storeLocalSession(res.result.extraData);
                    }
                });
            },
            doToggleMenu: () => {
                const close = $("body").hasClass(modal.classSideBar);
                if (!close) {
                    $(modal.classToggle).addClass("close");
                    $("body").addClass(modal.classSideBar);
                    $("header").addClass(modal.clazzHeader)
                    $(modal.idDraw).css("transform", "none");
                } else {
                    $(modal.classToggle).removeClass("close");
                    $("body").removeClass(modal.classSideBar);
                    $("header").removeClass(modal.clazzHeader)
                    $(modal.idDraw).css("transform", "translateX(-250px)");
                }
            },
            loadActive: () => {
                if (window.location.pathname === '/trang-chu') {
                    $(modal.navBar).addClass("active");
                    $(modal.navHeaderBar).addClass("active");
                }
                if (false) {
                    modal.showNotification("", modal.notificationInfo)
                }
            },
            doFacebookSignIn: () => modal.call(modal.typeFacebook, modal.urlFacebook),
            doGoogleSignIn: () => modal.call(modal.typeGoogle, modal.urlGoogle),
            showNotification: (msg, clazz, timeout = 4000) => {
                $(modal.notificationContainer).children("div").append(modal.addNotication(msg, clazz));
                const notification = $(modal.notification).length;
                if (notification > 0) {
                    setTimeout(() => $($(modal.notification)[notification - 1]).addClass("notification-enter-active"), 100);
                }
                setTimeout(() => modal.hideNotification(), timeout);
            },
            hideNotification: () => {
                const notification = $(modal.notification).length;
                if (notification > 0) {
                    $($(modal.notification)[0]).addClass(modal.notificationLeave);
                    setTimeout(() => $($(modal.notification)[0]).remove(), 500);
                }
            },
            showPush: (data) => {
                $(modal.notificationServerContainer).children("div.push").append(modal.addServerNotification(data));
                const notification = $(modal.notificationServer).length;
                if (notification > 0) {
                    setTimeout(() => $($(modal.notificationServer)[notification - 1]).addClass("notification-enter-active"), 100);
                }
                setTimeout(() => modal.hidePushTimeout(), 300000);
            },
            hidePushTimeout: () => {
                const notification = $(modal.notificationServer).length;
                if (notification > 0) {
                    $($(modal.notificationServer)[0]).addClass(modal.notificationLeave);
                    setTimeout(() => $($(modal.notificationServer)[0]).remove(), 300);
                }
            },
            hidePush: (_self) => {
                $(_self).parent().addClass(modal.notificationLeave);
                setTimeout(() => $(_self).parent().remove(), 300);
            },
            closePush: (event) => {
                event.preventDefault();
            },
            addNotication: (message, clazz) => {
                return '<div class="notification notification-enter ' + clazz + '"> <div class="notification-message" role="alert"> <div class="message">' + message + '</div> </div> </div>'
            },
            addServerNotification: (data) => {
                return '<div class="notification-server notification-enter"><a class="notification-message-server" href="' + data.url + '" target="_blank"  rel="noopener noreferrer" onClick="modal.hidePush(this)" role="alert"><div class="image">' +
                    '<img src="' + data.image + '" alt="' + data.image + '"></div><div class="push-content"><div class="push-title">' + data.title + '</div><div class="push-chapter">Chương mới: ' + data.chapter + '</div>' +
                    '<div class="push-time">Lúc:' + data.time + '</div><div class="push-author">Từ: ' + data.author + '<span onClick="modal.closePush(event)">X<span></div></div></a></div>';
            },
            isAnonymous: () => {
                debugger;
            },
            checkNotFound: () => {
                const msgError = '';
                if (!!msgError) {
                    modal.showNotification(msgError, modal.notificationError);
                }
            },
            drawReminderHtml: (data, msg = false) => {
                const mentionUser = "<span>{0}</span> đã nhắc tới ban <span>{1}</span>".replace("{0}", data.commenter).replace("{1}", data.reminder);
                const url = msg ? "#" : `/truyen/` + data.comicNameEn + '?commentId=' + data.commentId;
                return `<div class="reminder-block" data-url= ` + url + `>
                <div class="comic">
                    <span class="title"><a href="` + url + `">` + data.comicName + `</a></span>
                </div>
                <span class="date">` + data.createdDate + `</span>
                <div class="reminder">` + mentionUser + `</div>
                <div class="content">` + data.content + `</div>
            </div>`
            },
            initUser: () => {
                $.ajax({
                    url: "/api/cleanSession",
                    type: 'POST'
                }).done(res => {
                    if (res.status && res.result) {
                        localStorage.clear();
                        localStorage.setItem("user_profile", "Empty");
                    }
                });
            },
            trackUser: (width, append = "", direct = false) => {
                $.ajax({
                    url: `/api/user/tracking`,
                    beforeSend: beforeAuth,
                    type: 'POST',
                    dataType: "json",
                    data: {
                        width: width,
                        name: !!localStorage.getItem("user_profile") + "|" + append,
                        number: "",
                        direct: direct
                    }
                }).done(res => {})
            },
            activeReminderPopup: () => {
                if ($(modal.modalMessage).hasClass("active")) {
                    $(modal.modalMessage).removeClass("active");
                    $("body").removeClass("no-scroll");
                } else {
                    modal.loadReminderData();
                }
            },
            activeChangeNamePopUp: () => {
                if ($(modal.modalChangeName).hasClass("active")) {
                    $(modal.modalChangeName).removeClass("active");
                    $("body").removeClass("no-scroll");
                } else {
                    modal.loadUserInformation();
                }
            },
            loadUserInformation: () => {
                $.ajax({
                    url: `/api/user/checkChangeName`,
                    beforeSend: beforeAuth,
                    type: 'GET'
                }).done(res => {
                    if (res.status) {
                        const data = res.result;
                        $(".change-name").val(data.name);
                        $(".change-title").val(data.title);
                        $(modal.modalChangeName).addClass("active");
                        $("body").addClass("no-scroll");
                        if (window.isMobile) {
                            modal.doToggleMenu();
                        }
                    } else {
                        modal.showNotification(res.messages[0], modal.notificationWarn);
                    }
                });
            },
            updateUserInformation: (_self) => {
                if ($(_self).hasClass("btn-disable")) {
                    return;
                }
                const isAgree = confirm('Bạn muốn thực hiện đổi thông tin?');
                if (isAgree) {
                    $(_self).addClass("btn-disable");
                    const name = $(".change-name").val();
                    const title = $(".change-title").val();
                    if ((name.includes("<") && name.includes(">")) || (title.includes("<") && title.includes(">"))) {
                        alert("Nội dung có chứa ký tự đặc biệt");
                        return;
                    }
                    $.ajax({
                        url: `/api/user/updateName`,
                        beforeSend: beforeAuth,
                        type: 'POST',
                        data: {
                            name: name,
                            title: title
                        }
                    }).done(res => {
                        if (res.status) {
                            const data = res.result;
                            const user = getUserData();
                            user.rank = data.rank;
                            user.name = data.name;
                            localStorage.setItem("user_info", JSON.stringify(user));
                            modal.showNotification(data.message, modal.notificationSuccess);
                            modal.activeChangeNamePopUp();
                        } else {
                            modal.showNotification(res.messages[0], modal.notificationWarn);
                        }
                        $(_self).removeClass("btn-disable");
                    });
                }
            },
            loadReminderData: () => {
                if ($(modal.modalMessage).hasClass("loaded")) {
                    $(modal.modalMessage).addClass("active");
                    return;
                }
                $.ajax({
                    url: `/api/user/getReminders`,
                    beforeSend: beforeAuth,
                    type: 'GET'
                }).done(res => {
                    if (res.status) {
                        const data = res.result;
                        for (let i = 0; i < data.length; i++) {
                            if (data[i].type === 'MESSAGE_USER') {
                                $(modal.clazzReminderBody).append(modal.drawReminderHtml(data[i], true));
                            } else {
                                $(modal.clazzReminderBody).append(modal.drawReminderHtml(data[i]));
                            }
                        }
                        $(modal.modalMessage).addClass("active").addClass("loaded");
                        $("body").addClass("no-scroll");
                    }
                });
            },
            showModalSearch: () => {
                $(modal.searchSuggestionMb).html("");
                $(modal.searchInputMb).val("");
                $(modal.modalSearch).show();
                setTimeout(() => {
                    $(modal.searchInputMb).focus();
                }, 500);
                $("body").addClass("no-scroll");
            },
            hideModalSearch: () => {
                $("body").removeClass("no-scroll");
                $(modal.modalSearch).hide();
            },
            loadDataPrivate: (_self) => {
                if ($(_self).hasClass("active")) {
                    return;
                }
                const isAgree = confirm('Bạn muốn gửi yêu cầu thăng cấp tài khoản?');
                if (isAgree) {
                    $(_self).addClass("active");
                    $.ajax({
                        url: `/api/user/upgrade`,
                        beforeSend: beforeAuth,
                        type: 'POST',
                        data: {
                            session: modal.getLocalSession(),
                            history: modal.getHistoryData()
                        }
                    }).done(res => {
                        if (res.status) {
                            modal.showNotification('Bạn đã thực hiện yêu cầu thăng cấp, vui lòng chờ kết quả từ phía quản trị', modal.notificationSuccess);
                        } else {
                            modal.showNotification(res.messages[0], modal.notificationWarn);
                        }
                        $(_self).removeClass("active");
                    });
                }
            },
            loadUserSessionData: () => {






            }
        };
    </script>
    <script src="/contents/js/push-init.js"></script>
    <script src="/contents/js/push-message.js"></script>


