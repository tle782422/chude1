    <script type="text/javascript">
        if (!!localStorage.getItem("mode")) {
            document.querySelector("body").classList.add("dark-mode");
            document.querySelector(".moon-icon").classList.add("active");
        } else {
            document.querySelector(".sun-icon").classList.add("active");
        }
        $(document).ready(function() {
            $("#notification").fadeIn("slow").append('your message');
            $(".dismiss").click(function() {
                $("#notification").fadeOut("slow");
            });
            modal.init();
            $(document).on("click", modal.clazzGoogle, () => {
                modal.doGoogleSignIn();
            });
            $(document).on("click", modal.clazzFacebook, () => {
                modal.doFacebookSignIn();
            });
            $(document).on("click", modal.clazzLogout, () => {
                modal.doSignOut();
            });
            $(document).on("click", modal.clazzLogoutMB, () => {
                modal.doSignOut();
            });
            $(document).on("click", modal.clazzLogoutMenu, () => {
                modal.doSignOut();
            });
            $(document).on("click", modal.btnCloseModalSearch, () => {
                modal.hideModalSearch();
            });
            $(document).on("click", modal.btnShowModalSearch, () => {
                modal.showModalSearch();
                modal.doToggleMenu();
            });
            $(document).on("click", modal.btnShowModalSearchHeader, () => {
                modal.showModalSearch();
            });
            $(document).on("click", modal.clazzUserSync, (e) => {
                e.preventDefault();
                const isAgree = confirm('Bạn có muốn thực hiện đồng bộ dữ liệu lên tài khoản?');
                if (isAgree) {
                    modal.doSyncPush();
                }
            });
            $(document).on("click", modal.clazzUserRestore, (e) => {
                e.preventDefault();
                const isAgree = confirm('Bạn muốn đồng bộ thông tin tài khoản xuống thiết bị?');
                if (isAgree) {
                    modal.doRestoreDeviceData();
                }
            });
            $(document).on("click", modal.clazzUserFollow, () => {
                window.location.href = '/truyen/theo-doi';
            });
            $(document).on("click", modal.clazzUserHistory, () => {
                window.location.href = '/truyen/luot-su';
            });
            $(document).on("click", modal.classActiveToggle, (e) => {
                e.stopPropagation();
                modal.doToggleMenu();
            });
            $(document).on("click", ".btn-login", () => {
                modal.open();
            });
            $(document).on("click", "#modalLogin", () => {
                modal.close();
            });
            $(document).on("click", modal.searchIcon, (e) => {
                e.stopPropagation();
                const val = $(e.target).parent().parent().find(modal.searchInput).val();
                modal.doSearch(val);
            });
            $(document).on("click", modal.searchIcon1, (e) => {
                e.stopPropagation();
                const val = $(e.target).parent().parent().find(modal.searchInput).val();
                modal.doSearch(val);
            });
            $(document).on("click", modal.clazzReminder, (e) => {
                e.stopPropagation();
                modal.activeReminderPopup();
            });
            $(document).on("click", modal.clazzReminderMB, (e) => {
                e.stopPropagation();
                modal.activeReminderPopup();
                modal.doToggleMenu();
            });
            $(document).on("click", modal.clazzCloseReminder, (e) => {
                e.preventDefault();
                modal.activeReminderPopup();
            });
            $(document).on("click", modal.clazzCloseChangeName, (e) => {
                e.preventDefault();
                modal.activeChangeNamePopUp();
            });
            $(document).on("click", modal.clazzReminderBlock, (e) => {
                e.preventDefault();
                const url = $(e.target).closest(modal.clazzReminderBlock).attr("data-url");
                if (url !== "#") {
                    window.open(url, '_blank').focus();
                }
            });
            $(document).on("keyup", modal.searchInputWeb, (e) => {
                const val = $(e.target).val();
                modal.preSearch(val)
            });
            $(document).on("keyup", modal.searchInputMb, (e) => {
                const val = $(e.target).val();
                modal.preSearchMb(val)
            });
            $(document).on("focusout", modal.searchInputWeb, (e) => {
                $(modal.searchSuggestion).removeClass("active");
            });
            $(document).on("keypress", modal.searchInput, (e) => {
                if (e.keyCode === 13) {
                    const val = $(e.target).val();
                    modal.doSearch(val);
                }
            });
            $(document).on("click", modal.clazzUserRequest, (e) => {
                e.preventDefault();
                modal.loadDataPrivate(e.target);
            });
            $(document).on("click", modal.clazzUserGroup, (e) => {
                e.preventDefault();
                modal.showUserInfoPanel();
            });
            $(document).on("click", modal.clazzUserRename, (e) => {
                e.preventDefault();
                modal.activeChangeNamePopUp();
            });
            $(document).on("click", modal.btnRename, (e) => {
                e.preventDefault();
                modal.updateUserInformation(e.target);
            });
            $(document).on("click", modal.classSunMode, (e) => {
                e.preventDefault();
                modal.toggleModeDisplay();
            });
            $(document).on("click", modal.classDarkMode, (e) => {
                e.preventDefault();
                modal.toggleModeDisplay();
            });

        });
        console.write = () => {
            txtLog = "start";
            console.profile(logText);
            txtLog = "end";
            console.profileEnd(logText);
        };
        const modal = {
            clazz: "#modalLogin",
            modalMessage: "#modalMessage",
            modalChangeName: "#modalChangeName",
            btnRename: ".change-name-input-section > .btn-rename",
            clazzFacebook: ".sign-in-fb",
            clazzGoogle: ".sign-in-ga",
            clazzReminderBlock: ".reminder-block",
            clazzLogout: ".user-section > .avatar > img",
            clazzLogoutMB: ".user-sign-out",
            clazzUserAction: ".login-tab > .user-action",
            clazzLogoutMenu: ".login-tab > .user-action > .user-logout",
            clazzReminder: ".login-tab > .user-action > .user-reminder",
            clazzUserRequest: ".user-request",
            clazzUserRename: ".user-rename",
            clazzReminderMB: ".nav-section .user-reminder",
            clazzReminderBody: "#modalMessage .modal-body",
            clazzCloseReminder: ".close-message",
            clazzCloseChangeName: ".close-popup-change-name",
            clazzUserSync: ".user-sync",
            clazzUserGroup: ".user-group",
            clazzUserRestore: ".user-restore",
            clazzUserRankMB: ".nav-section .user-rank",
            clazzUserRequestMB: ".nav-section .user-request",
            clazzUserRankTxt: '.nav-section .user-rank .txt',
            clazzUserFollow: ".login-tab > .user-action > .user-follow",
            clazzUserHistory: ".login-tab > .user-action > .user-history",
            urlFacebook: "/api/login/facebook",
            urlGoogle: "/api/login/google",
            loginTab: ".login-tab",
            adminTab: ".admin-tab",
            typeFacebook: "FB",
            typeGoogle: "GA",
            classSideBar: "side-bar-menu",
            clazzHeader: "header-menu",
            clazzDrawOpen: "drawer-open",
            clazzDrawClose: "drawer-close",
            idDraw: "#drawer",
            classToggle: ".menu-toggle",
            classActiveToggle: '.active-toggle',
            isOpen: false,
            searchInputWeb: ".search-input-web",
            searchInputMb: ".modal-search > .modal-content-search > .modal-body-search > input",
            currentSearch: undefined,
            searchSuggestion: ".search-suggestion",
            searchSuggestionMb: ".modal-search > .modal-content-search > .modal-result-search",
            searchInput: "input.search-input",
            searchIcon: ".search-icon > svg",
            searchIcon1: ".search-icon",
            notificationContainer: ".notification-container",
            notification: ".notification",
            notificationServerContainer: ".notification-container-server",
            notificationServer: ".notification-server",
            notificationLeave: "notification-leave notification-leave-active",
            notificationEnter: "notification-enter notification-enter-active",
            notificationSuccess: "notification-success",
            notificationError: "notification-error",
            notificationInfo: "notification-info",
            notificationWarn: "notification-warning",
            notifiationMessage: ".notification-message > .message",
            navBar: ".nav-section > li.home-panel > a",
            navHeaderBar: "nav > ul > li:nth-of-type(2) > a",
            modalSearch: ".modal-search",
            btnSearch: ".btn-search",
            btnCloseModalSearch: ".btn-close-search",
            btnShowModalSearch: ".btn-search-modal",
            btnShowModalSearchHeader: ".btn-search-header",
            classSunMode: ".sun-icon",
            classDarkMode: ".moon-icon",
            init: () => {
                // modal.loadUserSessionData();
                modal.checkLogin();
                modal.loadActive();
                modal.tokenExpire();
                modal.checkNotFound();
                modal.checkReportFixed();
                modal.loadReminder();
            },
            tokenExpire: () => {
                if (!!localStorage.getItem("expired")) {
                    localStorage.removeItem("expired");
                    modal.showNotification('Phiên làm việc của bạn đã hết hạn. ', modal.notificationError);
                }
            },
            bannedUser: (content, redirect = true) => {
                $.ajax({
                    url: `/api/user/checked`,
                    beforeSend: beforeAuth,
                    type: 'POST',
                    dataType: "json",
                    data: {
                        content: content
                    }
                }).done(res => {
                    if (res !== null && res.status && redirect) {
                        alert(res.result.msg);
                        window.location.href = res.result.url;
                    }
                })
            },
            loadReminder: () => {
                const user = getUserData();
                if (modal.isAuthenticated(user)) {
                    $.ajax({
                        url: `/api/user/getCountReminder`,
                        beforeSend: beforeAuth,
                        type: 'GET'
                    }).done(res => {
                        if (res !== null && res.status && res.result > 0) {
                            if (window.isMobile) {
                                $(modal.classToggle).prepend("<span class='reminder'>" + res.result + "</span>");
                                $(modal.clazzReminderMB).find(".txt").append(` (` + res.result + `)`)
                                $(modal.clazzReminderMB).show();
                            } else {
                                const tagMessage = `<div class="action user-reminder"><span class="icon"><svg class="MuiSvgIcon-root MuiSvgIcon-fontSizeMedium MuiSvgIcon-root MuiSvgIcon-fontSizeLarge css-zjt8k" focusable="false" viewBox="0 0 24 24" aria-hidden="true" data-testid="MailOutlineIcon" tabindex="-1" title="MailOutline"><path d="M20 4H4c-1.1 0-1.99.9-1.99 2L2 18c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V6c0-1.1-.9-2-2-2zm0 14H4V8l8 5 8-5v10zm-8-7L4 6h16l-8 5z"></path></svg></span><span class="txt">Thông Báo (` + res.result + `)</span></div>`
                                $(modal.loginTab).append("<span class='reminder'>" + res.result + "</span>");
                                $(modal.clazzUserAction).prepend(tagMessage)
                            }
                        }
                    })
                }
            },
            redirect: (url) => {
                var return_path = window.location.href;
                localStorage.setItem("return_path", return_path);
                window.location.href = url;
            },
            doSearch: (value) => {
                if (!value || value.length < 3) {
                    modal.showNotification('Cần nhập từ khóa tìm kiếm với chiều dài hơn 3.', modal.notificationWarn);
                    return;
                }
                localStorage.setItem("keyword", value);
                const url = '/danh-sach';
                window.location.href = url;
            },
            preSearch: (value) => {
                if (modal.currentSearch !== undefined) {
                    clearTimeout(modal.currentSearch);
                }
                if (!value || value.length < 3) {
                    return;
                }
                modal.currentSearch = setTimeout(() => {
                    $.ajax({
                        url: "/api/comic/search",
                        type: 'GET',
                        dataType: "json",
                        data: {
                            name: value
                        }
                    }).done(res => {
                        if (res.status && res.result.length > 0) {
                            const data = res.result;
                            $(modal.searchSuggestion).html("").addClass("active");
                            for (const index in data) {
                                $(modal.searchSuggestion).append(modal.renderHtmlItem(data[index]));
                            }
                        } else {
                            $(modal.searchSuggestion).html("").removeClass("active");
                        }
                    });
                }, 1000);
            },
            preSearchMb: (value) => {
                if (modal.currentSearch !== undefined) {
                    clearTimeout(modal.currentSearch);
                }
                if (!value || value.length < 3) {
                    return;
                }
                modal.currentSearch = setTimeout(() => {
                    $.ajax({
                        url: "/api/comic/search",
                        type: 'GET',
                        dataType: "json",
                        data: {
                            name: value
                        }
                    }).done(res => {
                        if (res.status && res.result.length > 0) {
                            const data = res.result;
                            $(modal.searchSuggestionMb).html("").addClass("active");
                            for (const index in data) {
                                $(modal.searchSuggestionMb).append(modal.renderHtmlItem(data[index]));
                            }
                        } else {
                            $(modal.searchSuggestionMb).html("").removeClass("active");
                        }
                    });
                }, 1000);
            },
            renderHtmlItem: (item) => {
                const urlDetail = '/truyen/' + item.nameEn;
                const urlView = urlDetail + "/chuong-" + item.chapterLatest[0];
                return '<div class="item"><a href="' + urlDetail + '" class="image"><img src="' + item.photo + '" /></a><div class="info">' +
                    '<a  href="' + urlDetail + '" class="name">' + item.name + '</a><a  href="' + urlView + '" class="chapter">Chương ' + item.chapterLatest[0] + '</a></div></div>';
            },
            open: () => {
                if (!modal.isOpen) {
                    modal.isOpen = true;
                    $(modal.clazz).show();
                }
            },
            close: () => {
                if (modal.isOpen) {
                    modal.isOpen = false;
                    $(modal.clazz).hide();
                }
            },
            call: (type, url) => {
                $.ajax({
                    url: url,
                    type: 'POST',
                    dataType: "json",
                    data: {
                        type
                    }
                }).done(res => {
                    if (res.status === true) {
                        modal.redirect(res.result);
                    } else {
                        modal.showNotification(res.messages[0], modal.notificationError);
                    }
                });
            },
            isAuthenticated: (user) => {
                return !(!user.token || !user.name || !user.type || !user.image);
            },
            showUserInfoPanel: () => {
                if ($(modal.clazzUserGroup).hasClass("group-active")) {
                    $(modal.clazzUserGroup).removeClass("group-active")
                } else {
                    $(modal.clazzUserGroup).addClass("group-active")
                }
            },
            toggleModeDisplay: () => {
                const isDarkMode = !!localStorage.getItem("mode");
                if (isDarkMode) {
                    localStorage.removeItem("mode");
                    $("body").removeClass("dark-mode");
                    $(modal.classDarkMode).removeClass("active");
                    $(modal.classSunMode).addClass("active");
                } else {
                    localStorage.setItem("mode", "mode");
                    $("body").addClass("dark-mode");
                    $(modal.classDarkMode).addClass("active");
                    $(modal.classSunMode).removeClass("active");
                }
            },
            checkLogin: () => {
                const user = getUserData();
                if (modal.isAuthenticated(user)) {
                    $(modal.clazzUserGroup).removeClass("hide");
                    $(modal.clazzLogoutMB).show();
                    $(modal.clazzUserRankTxt).append(user.rank + 1);
                    if (user.admin || user.team) {
                        $(modal.adminTab).show();
                    }
                    var html = "<div class='avatar'><img src='" + user.image + "' alt='" + user.name + "' /><span class='name'>" + user.name + "</span></div>";
                    if (!window.isMobile) {
                        html += '<div class="user-action">' +
                            '                        <div class="action user-name">' +
                            '                            <span class="icon">' +
                            '                                <svg>' +
                            '                                    <path d="M12 12c2.21 0 4-1.79 4-4s-1.79-4-4-4-4 1.79-4 4 1.79 4 4 4zm0 2c-2.67 0-8 1.34-8 4v2h16v-2c0-2.66-5.33-4-8-4z"></path>' +
                            '                                </svg>' +
                            '                            </span>' +
                            '                            <span class="txt">' + user.name + '</span>' +
                            '                        </div>' +
                            '                        <div class="action user-rank">' +
                            '                            <span class="icon">' +
                            '                                <svg>' +
                            '                                    <path d="M17 10.43V2H7v8.43c0 .35.18.68.49.86l4.18 2.51-.99 2.34-3.41.29 2.59 2.24L9.07 22 12 20.23 14.93 22l-.78-3.33 2.59-2.24-3.41-.29-.99-2.34 4.18-2.51c.3-.18.48-.5.48-.86zm-4 1.8-1 .6-1-.6V3h2v9.23z"></path>' +
                            '                                </svg>' +
                            '                            </span>' +
                            '                            <span class="txt">Cấp Độ: ' + (user.rank + 1) + '</span>' +
                            '                        </div>' +
                            '                        <div class="action user-rename">' +
                            '                            <span class="icon">' +
                            '                                <svg>' +
                            '                                    <path d="M18.41 5.8 17.2 4.59c-.78-.78-2.05-.78-2.83 0l-2.68 2.68L3 15.96V20h4.04l8.74-8.74 2.63-2.63c.79-.78.79-2.05 0-2.83zM6.21 18H5v-1.21l8.66-8.66 1.21 1.21L6.21 18zM11 20l4-4h6v4H11z"></path>' +
                            '                                </svg>' +
                            '                            </span>' +
                            '                            <span class="txt">Đổi Tên Hiển Thị</span>' +
                            '                        </div>' +
                            '                        <div class="action user-request">' +
                            '                            <span class="icon">' +
                            '                                <svg>' +
                            '                                    <path d="m13.7826 15.1719 2.1213-2.1213 5.9963 5.9962-2.1213 2.1213zM17.5 10c1.93 0 3.5-1.57 3.5-3.5 0-.58-.16-1.12-.41-1.6l-2.7 2.7-1.49-1.49 2.7-2.7c-.48-.25-1.02-.41-1.6-.41C15.57 3 14 4.57 14 6.5c0 .41.08.8.21 1.16l-1.85 1.85-1.78-1.78.71-.71-1.41-1.41L12 3.49c-1.17-1.17-3.07-1.17-4.24 0L4.22 7.03l1.41 1.41H2.81l-.71.71 3.54 3.54.71-.71V9.15l1.41 1.41.71-.71 1.78 1.78-7.41 7.41 2.12 2.12L16.34 9.79c.36.13.75.21 1.16.21z"></path>' +
                            '                                </svg>' +
                            '                            </span>' +
                            '                            <span class="txt">Yêu Cầu Thăng Cấp</span>' +
                            '                        </div>' +
                            '                        <div class="action user-sync">' +
                            '                            <span class="icon">' +
                            '                                <svg>' +
                            '                                    <path d="M12 4V1L8 5l4 4V6c3.31 0 6 2.69 6 6 0 1.01-.25 1.97-.7 2.8l1.46 1.46C19.54 15.03 20 13.57 20 12c0-4.42-3.58-8-8-8zm0 14c-3.31 0-6-2.69-6-6 0-1.01.25-1.97.7-2.8L5.24 7.74C4.46 8.97 4 10.43 4 12c0 4.42 3.58 8 8 8v3l4-4-4-4v3z"></path>' +
                            '                                </svg>' +
